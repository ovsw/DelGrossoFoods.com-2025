{
  "name": "studio-dgf",
  "private": true,
  "version": "1.20.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "deploy-graphql": "sanity graphql deploy",
    "extract": "sanity schema extract --enforce-required-fields",
    "check-types": "tsc --noEmit",
    "check": "npm run lint && npm run format && npm run check-types",
    "check:fix": "npm run lint:fix && npm run format:fix",
    "type": "sanity schema extract --enforce-required-fields && sanity typegen generate",
    "postinstall": "sanity exec scripts/cli-alert-for-data.ts",
    "mut:delete-by-type": "sanity exec mutations/delete-by-type.ts --with-user-token"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/assist": "^5.0.4",
    "@sanity/icons": "^3.7.4",
    "@sanity/orderable-document-list": "^1.4.2",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.5.0",
    "@workspace/sanity-schema": "workspace:*",
    "lucide-react": "^0.539.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sanity": "^5.5.0",
    "sanity-plugin-asset-source-unsplash": "^7.0.2",
    "sanity-plugin-icon-picker": "^4.0.0",
    "sanity-plugin-media": "^4.1.1",
    "sanity-plugin-mux-input": "^2.13.0",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^19.2",
    "eslint": "^9.28",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.5",
    "typescript-eslint": "^8.41.0"
  }
}
